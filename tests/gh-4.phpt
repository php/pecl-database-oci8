--TEST--
Bug GH-4 (Memory leak with long query/CLOB)
--EXTENSIONS--
oci8
--SKIPIF--
<?php
require_once 'skipifconnectfailure.inc';
?>
--FILE--
<?php

require __DIR__.'/connect.inc';

$sql = 'select CONCAT(CONCAT(TO_CLOB(\'atk__binary__u5f8mzx4vsm8g2c9__\'), TO_CLOB(\'\')), CONCAT(TO_CLOB(\'\'), CONCAT(TO_CLOB(\'\'), TO_CLOB(\'2fb97249b20d0ff6c58506443c8cc9f68543f6e5a4dd295d6f95e7249f18d1ada91166276f5e682398635a9eaa1ba8aeea5b6ab10c4020680afc00ce4dc782f7c2be0\')))) AS "v" from "DUAL"';
$expectedStr = 'atk__binary__u5f8mzx4vsm8g2c9__';

$memUsages = array_flip(range(0, 100 - 1));
foreach (array_keys($memUsages) as $k) {
    $stid = oci_parse($c, $sql);
    oci_execute($stid);
    $row = oci_fetch_array($stid, \OCI_ASSOC);
    $res = $row['v']->load();
    if ($res !== $expectedStr) {
        var_dump([$expectedStr, $res]);
        throw new \Exception('unexpected result');
    }

    $memUsages[$k] = memory_get_usage();
}

$memUsages = array_slice($memUsages, 1, null, true);
$memUsages = array_unique($memUsages);

if (count($memUsages) !== 1) {
    var_dump($memUsages);
    throw new \Exception('memory leak detected');
}

echo "ok\n";

?>
--EXPECTF--
ok
